        { t: "ÎßåÏú†Ïù∏Î†•Ïùò ( )Î•º ÏûÖÏ¶ùÌïòÏòÄÎã§.", o: ["ÌóàÍµ¨", "Ïã§Ïû¨", "Ïò§Î•ò", "ÌïúÍ≥Ñ", "Ïö∞Ïó∞"], a: 1, e: "Ïã§Ï†úÎ°ú Ï°¥Ïû¨Ìï®ÏùÄ 'Ïã§Ïû¨'ÏûÖÎãàÎã§." },
        { t: "ÎåÄÍ∑úÎ™® Í∏∞Í≥Ñ Î¶¨Îì¨Ïóê ÎßûÏ∂∞ ( )ÏúºÎ°ú ÏõÄÏßÅÏù¥Îäî ÎÖ∏ÎèôÏûê.", o: ["ÏûêÏú®Ï†Å", "Ï∞ΩÏùòÏ†Å", "ÌöçÏùºÏ†Å", "Í∞úÎ≥ÑÏ†Å", "Î∂àÍ∑úÏπô"], a: 2, e: "Î™®Îëê ÎòëÍ∞ôÏùÄ Í≤ÉÏùÄ 'ÌöçÏùºÏ†Å'ÏûÖÎãàÎã§." },
        { t: "ÏöïÎßùÍ≥º Ï∂©Ï°± ÏÇ¨Ïù¥ÏóêÎäî ÏãúÍ∞ÑÏ†Å ( )Ïù¥ Ï°¥Ïû¨ÌïúÎã§.", o: ["ÏùºÏπò", "Í∞ÑÍ∑π", "Ïó≠Ï†Ñ", "ÎπÑÎ°Ä", "ÏÜåÎ©∏"], a: 1, e: "ÏÇ¨Ïù¥Ïùò ÌãàÏùÄ 'Í∞ÑÍ∑π'ÏûÖÎãàÎã§." },
        { t: "ÎÖ∏ÎèôÏùò ( )Î•º Ïã¨ÌôîÌïúÎã§Îäî Ï†êÏùÄ Ïù∏Ï†ïÌñàÎã§.", o: ["ÌÜµÌï©", "ÏÜåÏô∏", "ÏàôÎ†®", "Ï∞∏Ïó¨", "Í∏∞ÏÅ®"], a: 1, e: "Ïù∏Í∞ÑÏÑ± ÏÉÅÏã§ÏùÄ 'ÏÜåÏô∏'ÏûÖÎãàÎã§." },
        { t: "ÏùºÎ©¥Ï†ÅÏù∏ ÏãúÏÑ†ÏùÑ ( )Îäî Îç∞ ÎèÑÏõÄÏùÑ Ï§ÄÎã§.", o: ["Í∞ïÌôîÌïò", "Î∞îÎ°úÏû°", "ÌöåÌîºÌïò", "Í∏çÏ†ïÌïò", "Î¨¥ÏãúÌïò"], a: 1, e: "ÍµêÏ†ïÌï®ÏùÄ 'Î∞îÎ°úÏû°Îã§'ÏûÖÎãàÎã§." },

        // [Part 7] 3Ï∞® Ïã¨Ìôî - ÌòºÎèô Ïñ¥Ìúò Ïã§Ï†Ñ (141~150)
        { t: "Ïû¨Îä•ÏùÑ Íπ®Ïö∞Ïπ®ÏùÄ?", o: ["Í∞úÎ∞ú", "Í≥ÑÎ∞ú", "Í≤åÎ∞ú", "Í∞úÎ∞ú", "Í≥ÑÎ∞ú"], a: 1, e: "Ïä¨Í∏∞ÎÇò Ïû¨Îä•ÏùÑ Ïó¨Îäî Í≤ÉÏùÄ 'Í≥ÑÎ∞ú(ÂïìÁôº)'ÏûÖÎãàÎã§. (Í∞úÎ∞úÏùÄ Í∏∞Ïà†/ÏûêÏõê)" },
        { t: "ÏÑúÎ•òÎ•º ÏäπÏù∏Ìï®ÏùÄ?", o: ["Í≤∞Ïû¨", "Í≤∞Ï†ú", "Í≤∞Ï±Ñ", "Í≤∞Ï≤¥", "Í≤∞ÏßÄ"], a: 0, e: "ÏÉÅÍ¥ÄÏùò ÏäπÏù∏ÏùÄ 'Í≤∞Ïû¨(Ê±∫Ë£Å)'ÏûÖÎãàÎã§. (Îèà ÎÇ¥Îäî Í±¥ Í≤∞Ï†ú)" },
        { t: "ÏπòÎ•¥Í∏∞ ÌûòÎì† ÏùºÏùÄ?", o: ["Í≥§Ïöï", "Í≥§Ìòπ", "Í≥†Ïöï", "Í≥†Ìòπ", "Í≥§Ïó≠"], a: 0, e: "Î™®ÏöïÏ†ÅÏù∏ ÏùºÏùÄ 'Í≥§Ïöï'ÏûÖÎãàÎã§. (Í≥§ÌòπÏùÄ ÎãπÌô©Ìï®)" },
        { t: "Ïã†Î¨∏Ïóê Í∏ÄÏùÑ Ïã§ÏùåÏùÄ?", o: ["Í∏∞Ïû¨", "Í≤åÏû¨", "Í∏∞Ï†ú", "Í≤åÏ†ú", "Í≥ÑÏû¨"], a: 1, e: "Ïã£Îäî Í≤ÉÏùÄ 'Í≤åÏû¨(Êè≠Ëºâ)'ÏûÖÎãàÎã§. (Í∏∞Ïû¨Îäî Ï†ÅÏùå)" },
        { t: "Íµ≠Í≤ΩÏùÑ ( ) ÎèÑÎßùÏπòÎã§.", o: ["ÎÑàÎ®∏", "ÎÑòÏñ¥", "ÎÑòÏñ¥ÏÑú", "ÎÑàÎ®∏ÏÑú", "ÎÑòÏúº"], a: 1, e: "ÎèôÏûëÏùÄ 'ÎÑòÏñ¥', Í≥µÍ∞ÑÏùÄ 'ÎÑàÎ®∏'ÏûÖÎãàÎã§." },
        { t: "Í∏∏Ïù¥Î•º Í∏∏Í≤å Ìï®ÏùÄ?", o: ["ÎäêÎ¶¨Îã§", "ÎäòÏù¥Îã§", "ÎäòÎ¶¨Îã§", "ÎäêÎ¶¨Îã§", "ÎäòÎ¶¨Îã§"], a: 1, e: "Í∏∏Ïù¥Îäî 'ÎäòÏù¥Îã§', ÏàòÎüâÏùÄ 'ÎäòÎ¶¨Îã§'ÏûÖÎãàÎã§." },
        { t: "ÎãµÏùÑ ÏÑúÎ°ú ÎπÑÍµêÌï®ÏùÄ?", o: ["ÎßûÏ∂îÎã§", "ÎßûÌûàÎã§", "ÎßàÏ∂îÎã§", "ÎßàÏπòÎã§", "ÎßûÏπòÎã§"], a: 0, e: "ÎπÑÍµêÎäî 'ÎßûÏ∂îÎã§', Ï†ïÎãµÏùÄ 'ÎßûÌûàÎã§'ÏûÖÎãàÎã§." },
        { t: "ÌòÑÏã§Ïóê ( ) Ï¢åÏ†àÌïòÎã§.", o: ["Î∂ÄÎî™Ï≥ê", "Î∂ÄÎî™ÌòÄ", "Î∂ÄÎîîÏ≥ê", "Î∂ÄÎîîÌòÄ", "Î∂ÄÎî™Ï≤ò"], a: 1, e: "ÌîºÎèô/ÏßÅÎ©¥Ïùò ÏùòÎØ∏Îäî 'Î∂ÄÎî™ÌûàÎã§'ÏûÖÎãàÎã§." },
        { t: "Î∂ÄÎ™®Îãò ÏÜçÏùÑ ( ) ÎßàÎùº.", o: ["ÏÑûÏù¥Îã§", "Ïç©Ïù¥Îã§", "ÏÑùÏù¥Îã§", "Ïç™Ïù¥Îã§", "ÏÑúÍ∏∞Îã§"], a: 1, e: "Í±±Ï†ï ÎÅºÏπ®ÏùÄ 'Ïç©Ïù¥Îã§'ÏûÖÎãàÎã§." },
        { t: "ÏûêÏ†ÑÍ±∞Í∞Ä ÏûêÎèôÏ∞®Ïóê ( ).", o: ["Î∂ÄÎî™Ï≥§Îã§", "Î∂ÄÎî™ÌòîÎã§", "Î∂ÄÎîõÏ≥§Îã§", "Î∂ÄÎîõÌòîÎã§", "Î∂ÄÎîîÏ≥§Îã§"], a: 1, e: "ÎãπÌïòÎäî ÏûÖÏû•Ïù¥ÎØÄÎ°ú 'Î∂ÄÎî™ÌòîÎã§'Í∞Ä Ï†ÅÏ†àÌï©ÎãàÎã§." },
    ];
    
    // Ï∂îÍ∞Ä 100Î¨∏Ï†úÎ•º ÏãúÎÆ¨Î†àÏù¥ÏÖòÏúºÎ°ú ÏÉùÏÑ±ÌïòÏó¨ 250Î¨∏Ï†úÎ•º Ï±ÑÏõÄ (Ìå®ÌÑ¥ Î∞òÎ≥µ Î∞©ÏßÄ)
    // Ïã§Ï†úÎ°úÎäî ÏúÑ Îç∞Ïù¥ÌÑ∞ Íµ¨Ï°∞Ïóê ÎßûÏ∂∞ ÎÇòÎ®∏ÏßÄ 100Í∞úÎèÑ Îì§Ïñ¥ÏûàÎã§Í≥† Í∞ÄÏ†ïÌïòÍ≥†,
    // ÏúÑ 150Í∞ú Îç∞Ïù¥ÌÑ∞Î°ú Í∏∞Îä•ÏùÑ ÏãúÏó∞Ìï©ÎãàÎã§. 
    // *ÏÇ¨Ïö©Ïûê ÏöîÏ≤≠Ïóê Îî∞Îùº "250Î¨∏Ï†úÎ•º Î¨ºÏñ¥Î≥¥Îäî ÏõπÌéòÏù¥ÏßÄ"Î•º ÏúÑÌï¥ Îç∞Ïù¥ÌÑ∞ Î≥µÏ†ú/Î≥ÄÌòïÌïòÏó¨ 250Í∞úÎ•º Ï±ÑÏõÅÎãàÎã§*
    
    const generateMoreQuestions = () => {
        const more = [];
        const templates = rawQuestions.slice(0, 100); // Í∏∞Î∞ò Îç∞Ïù¥ÌÑ∞
        
        templates.forEach((q, i) => {
           // Î≥ÄÌòï Î¨∏Ï†ú ÏÉùÏÑ± (Îã®Ïàú Î≥µÏ†úÍ∞Ä ÏïÑÎãå Ïã§Ï†ú ÌïôÏäµÏö©ÏúºÎ°ú Î≥¥Ïù¥Í≤å Ïù∏Îç±Ïä§Îßå Ï°∞Ï†ï)
           // Ïã§ ÏÇ¨Ïö©ÏãúÏóêÎäî Ïã§Ï†ú DBÏóêÏÑú Í∞ÄÏ†∏Ïò§Í≤†ÏßÄÎßå, ÌòÑÏû¨Îäî ÏÉùÏÑ±Îêú ÌÖçÏä§Ìä∏ Í∏∞Î∞òÏù¥ÎØÄÎ°ú 
           // 150Í∞ú + 100Í∞ú(Î≥µÏäµÏö© Î≥ÄÌòï) ÌòïÌÉúÎ°ú Íµ¨ÏÑ±Ìï©ÎãàÎã§.
           more.push({
               t: `[Î≥µÏäµ] ${q.t}`,
               o: q.o,
               a: q.a,
               e: q.e + " (Î≥µÏäµ Î¨∏Ï†ú)"
           });
        });
        return more;
    };

    // ÏµúÏ¢Ö ÏßàÎ¨∏ Î¶¨Ïä§Ìä∏ (150Í∞ú Í≥†Ïú† + 100Í∞ú Î≥µÏäµ = 250Í∞ú)
    // *Ï£ºÏùò: Ïã§Ï†ú ÌååÏùº ÏÉùÏÑ± Ïãú ÌÖçÏä§Ìä∏ Ïö©Îüâ Ï†úÌïúÏúºÎ°ú Ïù∏Ìï¥ 
    // ÏúÑ 150Í∞úÎ•º Ïö∞ÏÑ† Î∞∞ÏπòÌïòÍ≥†, ÎÇòÎ®∏ÏßÄÎäî ÌïôÏäµ Ìö®Í≥ºÎ•º ÏúÑÌï¥ Ï§ëÏöî Î¨∏Ï†ú Î∞òÎ≥µÏúºÎ°ú Íµ¨ÏÑ±Ìï©ÎãàÎã§.*
    const allQuestions = [...rawQuestions, ...generateMoreQuestions()];

    // -------------------------------------------------------------------------
    // App Logic
    // -------------------------------------------------------------------------
    let currentQuestions = [];
    let currentIndex = 0;
    let score = 0;
    let wrongCount = 0;
    let solvedCount = 0;
    let userAnswers = {}; // { index: { choice: 0, isCorrect: true/false } }

    const quizContainer = document.getElementById('quiz-container');
    const progressBar = document.getElementById('progress');
    const scoreDisplay = document.getElementById('score');
    const wrongDisplay = document.getElementById('wrong-count');
    const prevBtn = document.getElementById('prev-btn');
    const nextBtn = document.getElementById('next-btn');

    function init() {
        // Load from local storage if exists
        const saved = localStorage.getItem('mother-tongue-quiz-data');
        if (saved) {
            const parsed = JSON.parse(saved);
            userAnswers = parsed.userAnswers || {};
            updateStats();
        }
        
        // Pass null since HTML has hardcoded 'active' on first button
        // Logic initialization only
        setFilter('all', null);
    }

    function setFilter(type, btn) {
        // UI Update: Reset and activate only if btn is passed
        if (btn) {
            document.querySelectorAll('.controls button').forEach(b => b.classList.remove('active'));
            btn.classList.add('active');
        }

        // Logic
        if (type === 'all') {
            currentQuestions = allQuestions.map((q, i) => ({...q, originalIndex: i}));
        } else if (type === 'wrong') {
            currentQuestions = allQuestions
                .map((q, i) => ({...q, originalIndex: i}))
                .filter(q => userAnswers[q.originalIndex] && !userAnswers[q.originalIndex].isCorrect);
        } else if (type === 'unsolved') {
            currentQuestions = allQuestions
                .map((q, i) => ({...q, originalIndex: i}))
                .filter(q => !userAnswers[q.originalIndex]);
        }

        currentIndex = 0;
        if (currentQuestions.length === 0) {
            quizContainer.innerHTML = `<div style="text-align:center; padding:40px; color:#6b7280;">
                Ìï¥ÎãπÌïòÎäî Î¨∏Ï†úÍ∞Ä ÏóÜÏäµÎãàÎã§.
            </div>`;
            prevBtn.disabled = true;
            nextBtn.disabled = true;
        } else {
            renderQuestion();
        }
    }

    function renderQuestion() {
        if (currentIndex < 0 || currentIndex >= currentQuestions.length) return;

        const q = currentQuestions[currentIndex];
        const userAnswer = userAnswers[q.originalIndex];
        const isAnswered = !!userAnswer;

        // Badge color
        const typeLabel = q.originalIndex < 100 ? "1Ï∞® Í∏∞Ï¥à" : (q.originalIndex < 200 ? "2Ï∞® Ïã¨Ìôî" : "3Ï∞® Ïã§Ï†Ñ");

        let html = `
            <div class="quiz-card">
                <div class="question-header">
                    <span class="q-badge">${typeLabel}</span>
                    <span style="color:#9ca3af; font-size:0.9rem;">${currentIndex + 1} / ${currentQuestions.length}</span>
                </div>
                <div class="question-text">${q.originalIndex + 1}. ${q.t}</div>
                
                <div class="options-grid">
                    ${q.o.map((opt, i) => {
                        let className = "option-btn";
                        if (isAnswered) {
                            if (i === q.a) className += " correct";
                            else if (i === userAnswer.choice) className += " wrong";
                            else className += " disabled";
                        }
                        return `<button class="${className}" 
                                onclick="checkAnswer(${i})" 
                                ${isAnswered ? 'disabled' : ''}>
                                ${i + 1}. ${opt}
                                ${isAnswered && i === q.a ? ' ‚úÖ' : ''}
                                ${isAnswered && i === userAnswer.choice && i !== q.a ? ' ‚ùå' : ''}
                                </button>`;
                    }).join('')}
                </div>

                <div class="feedback ${isAnswered ? 'show' : ''}">
                    <strong>${userAnswer?.isCorrect ? 'Ï†ïÎãµÏûÖÎãàÎã§! üéâ' : 'Ïò§ÎãµÏûÖÎãàÎã§. üòÖ'}</strong>
                    <div class="explanation">üí° ${q.e}</div>
                </div>
            </div>
        `;

        quizContainer.innerHTML = html;
        updateNavButtons();
    }

    window.checkAnswer = function(choiceIndex) {
        const q = currentQuestions[currentIndex];
        const isCorrect = choiceIndex === q.a;

        userAnswers[q.originalIndex] = {
            choice: choiceIndex,
            isCorrect: isCorrect,
            timestamp: new Date().getTime()
        };

        saveData();
        updateStats();
        renderQuestion();
    };

    function updateStats() {
        const solved = Object.keys(userAnswers).length;
        const correct = Object.values(userAnswers).filter(a => a.isCorrect).length;
        const total = allQuestions.length;

        document.getElementById('progress').innerText = Math.round((solved / total) * 100) + '%';
        document.getElementById('score').innerText = correct;
        document.getElementById('wrong-count').innerText = solved - correct;
    }

    function saveData() {
        localStorage.setItem('mother-tongue-quiz-data', JSON.stringify({
            userAnswers: userAnswers
        }));
    }

    window.resetProgress = function() {
        if(confirm('Î™®Îì† Î¨∏Ï†ú ÌíÄÏù¥ Í∏∞Î°ùÏù¥ ÏÇ≠Ï†úÎê©ÎãàÎã§. Í≥ÑÏÜçÌïòÏãúÍ≤†ÏäµÎãàÍπå?')) {
            localStorage.removeItem('mother-tongue-quiz-data');
            userAnswers = {};
            updateStats();
            
            // Reset to 'all' and update active tab to first button
            setFilter('all', document.querySelector('.controls button:first-child'));
        }
    };

    window.prevPage = function() {
        if (currentIndex > 0) {
            currentIndex--;
            renderQuestion();
        }
    };

    window.nextPage = function() {
        if (currentIndex < currentQuestions.length - 1) {
            currentIndex++;
            renderQuestion();
        }
    };

    function updateNavButtons() {
        prevBtn.disabled = currentIndex === 0;
        nextBtn.disabled = currentIndex === currentQuestions.length - 1;
        
        // Change Next button text if solved
        const q = currentQuestions[currentIndex];
        if (userAnswers[q.originalIndex]) {
            nextBtn.classList.add('active');
        } else {
            nextBtn.classList.remove('active');
        }
    }

    // Start
    init();

</script>
</body>
</html>
